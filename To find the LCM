import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    System.out.print("Enter the no.:");
	    int a = s.nextInt();
	    System.out.print("Enter the no.:");
	    int b = s.nextInt();
	    int big = a>b?a:b;
	    int lcm = big;
	    
	    while(true){
	        if(lcm%a == 0 && lcm%b == 0)
	            break;
	            else
	                lcm = big + lcm;
	       
	    }
		System.out.println("LCM IS:" + lcm);
	}
}
